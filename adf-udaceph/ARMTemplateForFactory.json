									"sourcepayrolldata": {},
									"sinkpayrollsynapse": {}
								}
							},
							"staging": {
								"linkedService": {
									"referenceName": "adls_udaceph",
									"type": "LinkedServiceReference"
								},
								"folderPath": "staging"
							},
							"compute": {
								"coreCount": 8,
								"computeType": "General"
							},
							"traceLevel": "Fine"
